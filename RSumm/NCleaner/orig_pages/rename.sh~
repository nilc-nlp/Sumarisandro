#!/bin/bash

for (( i=1; i<=10; i++ ))
do
	mv "e_bbc_$i.txt" "e_bbc_$i.html"
	mv "e_epoca_negocios_$i.txt" "e_epoca_negocios_$i.html"
	mv "e_exame_$i.txt" "e_exame_$i.html"
	mv "e_noticias_minuto_$i.txt" "e_noticias_minuto_$i.html"
	mv "e_terra_$i.txt" "e_terra_$i.html"
	mv "e_uol_$i.txt" "e_uol_$i.html"
	mv "e_acidade_$i.txt" "e_acidade_$i.html"
	mv "e_diariosp_$i.txt" "e_diariosp_$i.html"
	mv "e_dpernambuco_$i.txt" "e_dpernambuco_$i.html"
	mv "e_g1_$i.txt" "e_g1_$i.html"
	mv "e_jb_$i.txt" "e_jb_$i.html"
	mv "e_migalhas_$i.txt" "e_migalhas_$i.html"
	mv "e_oglobo_$i.txt" "e_oglobo_$i.html"
	mv "e_opovo_$i.txt" "e_opovo_$i.html"
	mv "e_zerohora_$i.txt" "e_zerohora_$i.html"
done
